@Styles.Render("~/MaterialDesign/plugins/sweetalert/sweetalert.css")
@Styles.Render("~/MaterialDesign/plugins/bootstrap-select/css/bootstrap-select.css")
@Scripts.Render("~/bundles/jquery")
@Scripts.Render("~/bundles/jqueryval")

@model FleeteInvoicing.Entities.Expanded
@{
    IEnumerable<FleeteInvoicing.Models.CFD_EXPANDED_ADDRESS> customers = TempData["Customers"] as IEnumerable<FleeteInvoicing.Models.CFD_EXPANDED_ADDRESS>;
    List<FleeteInvoicing.Models.CAT_PAYMENTMETHODTYPE> paymentlist = ViewBag.ListPayment;
}
<style type="text/css">
    #layoutFleet {
        margin-left: 0%;
    }
</style>

<div align="left">
    <div id="vmProvider" class="container-fluid">
        <div class="block-header">
            <h2><b>Catálogo de Clientes</b></h2>
        </div>
        <div class="row clearfix">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <div class="card">
                    <div class="header">
                        <h2>
                            @Html.ActionLink("Crear Cliente", "#", null, new { data_toggle = "modal", data_target = "#modProviderNew" })
                        </h2>
                    </div>
                    <div class="panel-body">
                        @using (Html.BeginForm("Index", "CFD_Expanded", FormMethod.Post, new { role = "form" })) { }
                    </div>
                    <div class="body">
                        <div id="area_tabla">
                            <table id="grdProvider" role="grid" class="table table-bordered table-striped table-hover dataTable">
                                <thead>
                                    <tr style="background-color:#757575;color:#fff;" role="row">
                                        <th class="sorting_asc" tabindex="0" aria-controls="DataTables_Table_1" rowspan="1" colspan="1" aria-sort="ascending" style="width: 150px;">#FLOTA</th>
                                        <th class="sorting_asc" tabindex="0" aria-controls="DataTables_Table_1" rowspan="1" colspan="1" aria-sort="ascending" style="width: 300px;">#CLIENTE</th>
                                        <th class="sorting" tabindex="0" rowspan="1" colspan="1" style="width:70px;">RFC</th>
                                        <th class="sorting" tabindex="0" rowspan="1" colspan="1" style="width:180px;">DIRECCION 1</th>
                                        <th class="sorting" tabindex="0" rowspan="1" colspan="1" style="width:180px;">DIRECCION 2</th>
                                        <th class="sorting" tabindex="0" rowspan="1" colspan="1" style="width:180px;">DIRECCION 3</th>
                                        <th class="sorting" tabindex="0" rowspan="1" colspan="1" style="width:100px;">CIUDAD</th>
                                        <th class="sorting" tabindex="0" rowspan="1" colspan="1" style="width:60px;">ESTADO</th>
                                        <th class="sorting" tabindex="0" rowspan="1" colspan="1" style="width:60px;">CODIGO POSTAL</th>
                                        <th class="sorting" tabindex="0" rowspan="1" colspan="1" style="width:30px;">METODO DE PAGO</th>
                                        <th class="sorting" tabindex="0" rowspan="1" colspan="1" style="width:450px;">ADMINISTRACION</th>
                                    </tr>
                                </thead>
                                <tbody>
                                   @foreach (var item in customers)
                                   {
                                    <tr>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.CUSTID)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.CUSTNAME)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.RFC)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.ADDRESS1)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.ADDRESS2)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.ADDRESS3)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.CITY)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.STATE)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.ZIP)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => paymentlist.Where(x => x.paymentMethodTypeCode == item.PAYMENT_METHOD_TYPE).FirstOrDefault().paymentMethodTypeCode) -
                                            @Html.DisplayFor(modelItem => paymentlist.Where(x => x.paymentMethodTypeCode == item.PAYMENT_METHOD_TYPE).FirstOrDefault().paymentMethodDesc)
                                        </td>
                                        <td>
                                            @{
                                                <button type="button" data-color="white" class="btn bg-blue-grey waves-effect" onclick='EditCustomer("@Html.DisplayFor(modelItem => item.CUSTID)")'>Editar</button>
                                            }

                                            @{
                                                <button type="button" data-color="red" class="btn bg-red" onclick='DeleteCustomer("@Html.DisplayFor(modelItem => item.CUSTID )")'>Eliminar</button>
                                            }

                                        </td>
                                    </tr>
                                  }
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

