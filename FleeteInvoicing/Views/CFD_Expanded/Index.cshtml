                    type: "warning",
                    closeOnCancel: false
                });
            }

        });

        function IsNumber() {
            key = (document.all) ? event.keyCode : event.which;
            if (key === 8) { return true; }
            Rex = /[0-9]/;
            KeyFin = String.fromCharCode(key);
            return Rex.test(KeyFin);
        }
    </script>

